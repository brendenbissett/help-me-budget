# OAuth Configuration
GOOGLE_KEY=your_google_oauth_client_id
GOOGLE_SECRET=your_google_oauth_client_secret
GOOGLE_CALLBACK_URL=http://localhost:5173/api/auth/callback/google

FACEBOOK_KEY=your_facebook_app_id
FACEBOOK_SECRET=your_facebook_app_secret
FACEBOOK_CALLBACK_URL=http://localhost:5173/api/auth/callback/facebook

# Database Configuration
# Option 1: Use full DATABASE_URL
DATABASE_URL=postgres://budgetuser:budgetpass@localhost:5432/help_me_budget?sslmode=disable

# Option 2: Use individual database variables (if DATABASE_URL not set)
DB_HOST=localhost
DB_PORT=5432
DB_USER=budgetuser
DB_PASSWORD=budgetpass
DB_NAME=help_me_budget
DB_SSLMODE=disable

# Redis Configuration
# Option 1: Use full REDIS_URL
REDIS_URL=redis://localhost:6379

# Option 2: Use individual Redis variables (if REDIS_URL not set)
REDIS_ADDR=localhost:6379
REDIS_PASSWORD=

# Application Environment
APP_ENV=development
